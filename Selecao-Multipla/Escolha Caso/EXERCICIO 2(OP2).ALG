   //Escreval("Digite o mes do seu nascimento:")
  // leia(mes)
  Escreval("1 - Janeiro")
  Escreval("2 - Fevereiro")
  Escreval("3 - Março")
  Escreval("4 - Abril")
  Escreval("5 - Maio")
  Escreval("6 - Junho")
  Escreval("7 - Julho")
